<?xml version="1.0"?>
<package>
  <metadata>
    <id>Exiled.Dev.References</id>
    <version>1.0.4</version>
    <authors>Swishhyy</authors>
    <owners>Swishhyy</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">MIT</license>
    <projectUrl>https://github.com/Swishhyy/Exiled_Dev_References</projectUrl>
    <description>NuGet Package for Exiled Plugin Development. This contains all .dlls you will need.</description>
    <readme>docs\README.md</readme>
    <tags>exiled plugin development dll libraries</tags>
    <repository type="git" url="https://github.com/Swishhyy/Exiled_Dev_References.git" />
    <dependencies>
      <group targetFramework=".NETFramework4.8" />
    </dependencies>
  </metadata>
  <files>
    <!-- Include all DLLs except Assembly-CSharp related ones -->
    <file src="binLibrary\*.dll" target="lib\net48" exclude="binLibrary\Assembly-CSharp.dll" />
    <!-- Include the README.md file in the package -->
    <file src="..\README.md" target="docs\" />
  </files>
</package>
